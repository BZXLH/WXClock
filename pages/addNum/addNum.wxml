<!--pages/addNum/addNum.wxml-->
<view class="container">
  <image src='../../image/background1.png' class="bg"></image>
  <icon class="finish-icon" type="success" size="70"></icon>
  <view class="show">
    <view class="new-plan">新建计划</view>
    <view class="plan-name">{{taskName}}</view>
    <view>这是你新建的第<text class="num">{{num}}</text>个计划</view>
    <button type="primary" class="back" bindtap="back">返回</button>
  </view>
</view>